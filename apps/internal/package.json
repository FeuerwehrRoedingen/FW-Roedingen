{
  "name": "fw-roedingen-internal",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "node --loader ts-node/esm server.ts",
    "build": "npm run build:client && npm run build:server",
    "build:client": "vite build --outDir dist/client",
    "build:server": "vite build --outDir dist/server --ssr src/entry-server.js",
    "generate": "vite build --outDir dist/static && npm run build:server && npm run prerender",
    "serve": "NODE_ENV=production node --loader ts-node/esm server.ts",
    "prerender": "node --loader ts-node/esm prerender.ts"
  },
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.4.2"
  },
  "devDependencies": {
    "@types/compression": "^1.7.2",
    "@types/react": "^18.0.17",
    "@types/react-dom": "^18.0.6",
    "@vitejs/plugin-react": "^2.1.0",
    "autoprefixer": "^10.4.12",
    "compression": "^1.7.4",
    "postcss": "^8.4.17",
    "serve-static": "^1.15.0",
    "tailwindcss": "^3.1.8",
    "typescript": "^4.6.4",
    "vite": "^3.1.0"
  }
}

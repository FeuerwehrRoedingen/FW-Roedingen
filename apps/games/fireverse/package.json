{
  "name": "fireverse",
  "version": "0.0.1",
  "description": "A Metaverse clone, but way cooler and hotter",
  "scripts": {
    "dev": "node server.js",
    "build:client": "vite build --outidr dist/react/client",
    "build:server": "vite build --outidr dist/react/server --ssr react/entry-server.ts",

    "asbuild:debug": "asc src/assembly/index.ts --target debug",
    "asbuild:release": "asc src/assembly/index.ts --target release",
    "asbuild": "npm run asbuild:debug && npm run asbuild:release",

    "start": "echo \"not implemented\""
  },
  "author": "Thomas DÃ¼ren",
  "license": "MIT",
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/build/release.js",
      "types": "./dist/build/release.d.ts"
    }
  },
  "devDependencies": {
    "@types/react": "^18.0.29",
    "@types/react-dom": "^18.0.11",
    "@vitejs/plugin-react": "^3.1.0",
    "vite": "^4.2.1"
  },
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  }
}

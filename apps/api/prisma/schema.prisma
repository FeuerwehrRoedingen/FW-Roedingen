
generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model User {
  id        Int      @id //from auth0
  fcmTokens Token[]
}

model Token {
  token  String @id
  userId Int
  user   User   @relation(fields: [userId], references: [id])
}
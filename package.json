{
  "name": "fw-roedingen",
  "version": "0.0.0",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*",
    "apps/api/functions"
  ],
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev --parallel",
    "lint": "turbo run lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "dev:browser": "concurrently \"cd apps/api && npm run dev\" \"cd apps/web && npm run dev\"",
    "dev:mobile": "concurrently \"cd apps/api && npm run dev\" \"cd apps/mobile && npm run dev\""
  },
  "devDependencies": {
    "concurrently": "^7.4.0",
    "eslint-config-custom": "*",
    "prettier": "latest",
    "turbo": "latest"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "packageManager": "npm@8.11.0"
}
